{
	"name": "afpa-wordpress",
	"private": false,
	"description": "AfPA's WordPress website with Gatsby",
	"version": "0.0.1",
	"author": "Avi Nerenberg <avinerenberg@gmail.com>",
	"license": "MIT",
	"scripts": {
		"start": "gatsby develop",
		"start:lambda": "netlify-lambda serve src/lambda",
		"build": "gatsby build",
		"build:lambda": "netlify-lambda build src/lambda",
		"serve": "gatsby serve",
		"format": "prettier --write src/**/*.{tsx}",
		"lint": "tslint src"
	},
	"prettier": {
		"endOfLine": "lf",
		"semi": false,
		"singleQuote": true,
		"useTabs": true,
		"trailingComma": "es5"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/avinoamsn/afpa-gatsby"
	},
	"bugs": {
		"url": "https://github.com/avinoamsn/afpa-gatsby/issues"
	},
	"keywords": [
		"gatsby",
		"AfPA",
		"Alliance",
		"Patient",
		"Access"
	],
	"dependencies": {
		"@gatsby-contrib/gatsby-plugin-elasticlunr-search": "^2.3.0",
		"@types/react-helmet": "^5.0.9",
		"autoprefixer": "^9.6.1",
		"elasticlunr": "^0.9.5",
		"eslint": "^6.6.0",
		"gatsby": "^2.18.17",
		"gatsby-background-image": "^0.8.3",
		"gatsby-cli": "^2.7.27",
		"gatsby-gravityforms-component": "^1.0.9",
		"gatsby-image": "^2.2.7",
		"gatsby-plugin-manifest": "^2.2.4",
		"gatsby-plugin-offline": "^2.2.4",
		"gatsby-plugin-postcss": "^2.1.3",
		"gatsby-plugin-purgecss": "^4.0.0",
		"gatsby-plugin-react-helmet": "^3.1.2",
		"gatsby-plugin-sharp": "^2.2.9",
		"gatsby-plugin-stylus-resources": "^1.0.24",
		"gatsby-plugin-tslint": "0.0.2",
		"gatsby-plugin-typescript": "^2.1.2",
		"gatsby-source-filesystem": "^2.1.9",
		"gatsby-source-gravityforms": "^1.0.15",
		"gatsby-source-wordpress": "^3.1.54",
		"gatsby-transformer-sharp": "^2.2.5",
		"netlify-lambda": "^1.6.3",
		"postcss-easy-import": "^3.0.0",
		"postcss-preset-env": "^6.7.0",
		"prop-types": "^15.7.2",
		"pure-react-carousel": "^1.21.2",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-helmet": "^5.2.1",
		"react-media-player": "^0.7.7",
		"react-player": "^1.11.2",
		"react-twitter-widgets": "^1.7.1",
		"rss-parser": "^3.7.2",
		"smooth-scroll": "^16.1.0",
		"sugarss": "^2.0.0",
		"typeface-montserrat": "0.0.75"
	},
	"devDependencies": {
		"@types/node": "^12.7.0",
		"@types/react": "^16.8.24",
		"@types/react-dom": "^16.8.5",
		"babel-eslint": "^10.0.2",
		"postcss-easing-gradients": "^3.0.1",
		"precss": "^4.0.0",
		"prettier": "^1.18.2",
		"tailwindcss": "^1.1.0",
		"tslint": "^5.18.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-loader": "^3.5.4",
		"tslint-plugin-prettier": "^2.0.1",
		"tslint-react": "^4.0.0",
		"typescript": "^3.5.3"
	}
}
